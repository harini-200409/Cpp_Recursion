#include <iostream>
using namespace std;
int largestElement(int arr[],int n)
{
    if(n==0)
    return arr[0];
    return max(arr[n-1],largestElement(arr,n-1));
}
int main()
{
    int n;
    cin>>n;
    int arr[n];
    for(int i=0;i<n;i++)
    {
        cin>>arr[i];
    }
    int res=largestElement(arr,n);
    cout<<res;
    return 0;
}
