#include <iostream>
using namespace std;
int linearSearch(int arr[],int n,int value,int index=0)
{
    if(index==n)
    return -1;
    if(arr[index]==value)
    return value;
    
    return linearSearch(arr,n,value,index+1);
}
int main()
{
   int n,value;
   cin>>n;
   int arr[n];
   for(int i=0;i<n;i++)
   {
       cin>>arr[i];
   }
   cin>>value;
   int res=linearSearch(arr,n,value);
    cout<<res;
    return 0;
}
