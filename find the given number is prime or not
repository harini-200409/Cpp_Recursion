#include <iostream>
using namespace std;
int Prime(int n)
{
    int prime=1;
    for(int i=2;i*i<=n;i++)
    {
        if(n%i==0)
        {
            prime=0;
            break;
        }
    }
    return prime;
}
int main()
{
    int n;
    cin>>n;
    int res=Prime(n);
    if(res)
    cout<<n<<" is a prime number";
    else
    cout<<n<<" is not a prime number";
    return 0;
}
